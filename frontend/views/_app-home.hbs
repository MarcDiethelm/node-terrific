

<!doctype html>
<html lang="en-US">
<head>
	<meta charset="UTF-8">
	<title>{{docTitle}}</title>
	<link rel="stylesheet" type="text/css" href="{{assets.css.external}}">
	<style>
		ul, li {
			margin: 0;
			padding: 0;
			list-style: none;
		}
		.item {
			padding-bottom: 1em;
		}
		.subitem {
			padding-top: .7em;
			padding-left: 1.7em;
		}
		.name {
			font-weight: bold;
		}
	</style>
</head>
<body>
	<h1>{{title}}</h1>
	<h2>Views</h2>

	<ul>
	{{#each views}}
		<li class="item">
			<a class="name" href="/_view/{{this.name}}">{{this.name}}</a>
			<small>
				 | <a href="/_view/{{this.name}}?raw">raw</a>{{#if this.repoUri}} | <a href="{{this.repoUri}}">git</a>{{/if}}
			</small>
		</li>
	{{/each}}
	</ul>


	<h2>Modules</h2>

	<ul>
	{{#each modules}}
		<li class="item">
			{{this.name}}
			<small>
				{{#if this.repoUri}} | <a href="{{this.repoUri}}">git</a>{{/if}}
				<ul>
				{{#each this.templates}}
					<li class="subitem">
						<a class="name" href="/_module/{{../this.name}}/{{this.name}}">{{this.name}}</a>
						| <a href="/_module/{{../this.name}}/{{this.name}}?raw">raw</a>
						{{#if this.repoUri}} | <a href="{{this.repoUri}}">git</a>{{/if}}
					</li>
				{{/each}}
				</ul>

			</small>
		</li>
	{{/each}}
	</ul>
</body>
</html>